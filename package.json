{
  "name": "cobra-cobol-agent",
  "version": "0.1.0",
  "description": "COBOL Banking Resilience Agent - AI-powered COBOL modernization system",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/mcp-server/index.ts",
    "mcp:start": "node dist/mcp-server/index.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:integration": "vitest --run --config vitest.integration.config.ts",
    "test:e2e": "npm run build && node dist/integration-test.js",
    "verify": "bash scripts/verify-integration.sh",
    "generate:lambda": "tsx src/cli/generate-lambda.ts",
    "generate:cdk": "tsx src/cli/generate-cdk.ts",
    "generate:spec": "tsx src/cli/generate-spec.ts",
    "web:dev": "cd src/web/frontend && npm run dev",
    "api:dev": "tsx watch src/web/backend/index.ts"
  },
  "keywords": [
    "cobol",
    "modernization",
    "aws",
    "mcp",
    "banking",
    "legacy-systems"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "cobol-parsers": "^1.0.12",
    "express": "^4.18.2",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
